{
  "name": "action-reducer",
  "version": "0.1.2",
  "description": "Type-safe ActionCreator and Reducer library",
  "main": "dist/action-reducer.cjs.js",
  "module": "dist/action-reducer.esm.js",
  "types": "dist/action-reducer.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:rollup && npm run build:rename",
    "build:rollup": "rollup -c",
    "build:rename": "mv dist/index.d.ts dist/action-reducer.d.ts",
    "test": "npm run test:types && npm run test:types-nullable && npm run test:mocha",
    "test:types": "tsc -p . --noEmit --strict",
    "test:types-nullable": "tsc -p . --noEmit --strict --strictNullChecks false",
    "test:mocha": "mocha --compilers ts:espower-typescript/guess tests/*.test.ts",
    "clean": "rm -r ./dist/*",
    "prepare": "npm run test && npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iMasanari/action-reducer.git"
  },
  "keywords": [
    "redux",
    "action creator",
    "ducks",
    "typescript"
  ],
  "author": "i-masanari",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.53",
    "@types/power-assert": "^1.4.29",
    "espower-typescript": "^8.1.2",
    "mocha": "^4.0.1",
    "power-assert": "^1.4.4",
    "rollup": "^0.52.0",
    "rollup-plugin-typescript2": "^0.8.4",
    "typescript": "^2.6.2"
  }
}
